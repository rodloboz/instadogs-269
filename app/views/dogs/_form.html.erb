<%= form_for(@dog) do |f| %>
  <%= f.label :name  %>
  <%= f.text_field :name %>

  <%= f.label :breed  %>
  <%= f.select :breed, collection: Dog::BREEDS %>

  <%= f.label :colour  %>
  <%= f.text_field :colour %>

  <%= f.label :birthdate  %>
  <%= f.text_field :birthdate %>

  <%= f.submit %>
<% end %>
